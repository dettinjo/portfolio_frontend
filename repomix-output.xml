"use client";

import { motion } from "framer-motion";

const gridVariants = {
  hidden: { opacity: 0 },
  visible: {
    opacity: 1,
    transition: {
      staggerChildren: 0.1, // Lässt die Kinder nacheinander erscheinen
    },
  },
};

